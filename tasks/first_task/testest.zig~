const std = @import("std");

pub fn main() !void {
//    const allocator = std.heap.page_allocator;

    const input = "42 84"; // Пример строки
    var parts: [2]u32 = undefined; // Массив для хранения двух целых чисел

    const result = std.mem.split(input, " ");
    var i: usize = 0;

    while (result.next()) |part| {
        if (i >= parts.len) {
            return error.TooManyParts; // Обработка случая, если частей больше двух
        }
        const parseResult = std.fmt.parseInt(u32, part);
        if (parseResult.isError()) {
            return parseResult; // Обработка ошибки преобразования
        }
        parts[i] = parseResult.?;
        i += 1;
    }

    // Теперь parts[0] и parts[1] содержат преобразованные значения
    std.debug.print("Первое число: {}\n", .{parts[0]});
    std.debug.print("Второе число: {}\n", .{parts[1]});
}
